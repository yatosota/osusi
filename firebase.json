{
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix functions install"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "auth": {
      "port": 9098
    },
    "firestore": {
      "port": 8081
    },
    "functions": {
      "port": 5002
    },
    "ui": {
      "enabled": true,
      "port": 4001
    },
    "hub": {
      "port": 4401
    },
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "pubsub": {
      "port": 8085
    },
    "storage": {
      "port": 9199
    },
    "eventarc": {
      "port": 9299
    },
    "dataconnect": {
      "port": 9399,
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    },
    "tasks": {
      "port": 9499
    },
    "singleProjectMode": true
  }
}
